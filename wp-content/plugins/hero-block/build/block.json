{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/hero-block",
  "version": "0.1.0",
  "title": "Hero Block",
  "category": "custom-category",
  "icon": "admin-comments",
  "description": "Hero block with large background image.",
  "attributes": {
    "heading_one": {
      "type": "string",
      "default": "HELLO WORLD"
    },
    "heading_two": {
      "type": "string",
      "default": "HELLO AGAIN"
    },
    "button": {
      "type": "string",
      "default": "Find out more"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "hero-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}